{
  "extends": "../../tsconfig.package-settings",
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "es2021"
    ],
    "module": "commonjs",
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "preserveValueImports": false,
  }
}